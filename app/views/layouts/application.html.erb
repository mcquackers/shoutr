<!DOCTYPE html>
<html>
<head>
  <title>Shoutr</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <style>
    h4{
      float: right;
    }
  </style>
</head>
<body>
  <h4><%= link_to "Sign up", new_user_path %>
  <%= current_user.email %></h4>

<%= yield %>

</body>
</html>
