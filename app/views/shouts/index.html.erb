<h2> SHOUTS </h2>
<%= render "error_messages", target: @shout %>
<%= form_for @shout do |form| %>
  <%= form.label "SHOUT!" %>
  <%= form.text_field :body %>
  <%= form.submit "SHOUT!" %>
<% end %>
<ul>
  <% @shouts.each do |shout| %>
    <li><%= shout.user_id %> - <%= shout.body %></li>
  <% end %>
</ul>
